{
  "name": "boundless",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "production": "dotenv-run-script .env .env.production --",
    "dev": "dotenv-run-script .env .env.dev --",
    "serve": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.dashboard.local.config.js vue-cli-service serve",
    "serve:remote": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.dashboard.local.config.js vue-cli-service --mode remote serve",
    "servelocal": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.dashboard.local.config.js vue-cli-service serve --mode servelocal",
    "serve:engine": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.portal.config.js vue-cli-service build src/engine/index.js --watch --mode servelocal",
    "engine:server": "",
    "test": "mocha-webpack --webpack-config webpack.test.config.js --require tests/unit/setup.js src/**/*.spec.js",
    "e2e": "cypress run",
    "lint": "vue-cli-service lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:record": "cypress run --record",
    "dev:dashboard": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.dashboard.config.js vue-cli-service build --mode dev",
    "production:dashboard": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.dashboard.config.js vue-cli-service build --mode production",
    "local:dashboard": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.dashboard.local.config.js vue-cli-service build --mode local",
    "portal:zip": "rm -f dist/portal.zip && cd dist/portal/ && bestzip ../portal.zip *; cd .. && rm -rf portal",
    "portallocal:zip": "rm -f dist/portal_localdocker.zip && cd dist/portal/ && bestzip ../portal_localdocker.zip *; cd .. && rm -rf portal",
    "portal:dev": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.portal.config.js vue-cli-service build src/engine/index.js --mode dev && npm run portal:zip",
    "portal:production": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.portal.config.js vue-cli-service build src/engine/index.js --mode production && npm run portal:zip",
    "portal:local": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.portal.config.js vue-cli-service build src/engine/index.js --mode local && npm run portallocal:zip",
    "portal:deploy": "curl -F 'file=@dist/portal.zip' --header \"Authorization: Api-Key $VUE_APP_API_KEY\" $VUE_APP_API_URL/portal-deployment/",
    "decker_production": "npm run decker_production:dashboard; npm run portal:decker_production",
    "local": "npm run dashboard:local; npm run portal:local",
    "test:e2e": "vue-cli-service test:e2e",
    "verify:locale": "node ./src/plugins/i18n/verify-locales/main.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
